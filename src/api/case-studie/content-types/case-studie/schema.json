      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "seo": {
      "type": "component",
      "repeatable": false,
      "component": "seo.shared-seo"
    },
    "relatedProducts": {
      "type": "component",
      "repeatable": false,
      "component": "case-products.case-products"
    }
